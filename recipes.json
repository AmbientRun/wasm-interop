{
    "build-app": {
        "cmd": [
            "wasm-pack",
            "build"
        ],
        "cwd": "app/"
    },
    "build-client": {
        "cwd": "./client",
        "cmd": [
            "wasm-pack",
            "build",
            "--target",
            "no-modules"
        ]
    },
    "serve": {
        "cmd": [
            "npm",
            "run",
            "dev"
        ],
        "cwd": "www/"
    }
}
